{
  "name": "@jeykheryendes/crestalink-test-frontend",
  "version": "1.0.0",
  "private": true,
  "author": "Jeykher Yendes <jeykher@gmail.com>",
  "description": "Yendes Softwares @ Next",
  "engines": {
    "node": ">=22.0.0",
    "bun": ">=1.2.0"
  },
  "keywords": [
    "Yendes Softwares",
    "Next",
    "Template"
  ],
  "license": "YENDES",
  "packageManager": "bun@1.3.2",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s clean compile",
    "check": "biome check",
    "clean:dependencies": "rimraf ./node_modules",
    "clean": "rimraf ./.next",
    "commitlint:check": "commitlint --from HEAD~1 --to HEAD --verbose",
    "commitlint": "commitlint --edit",
    "compile": "next build",
    "dev": "next dev",
    "format": "biome format --write",
    "husky": "husky",
    "lint": "biome lint --write",
    "security:scan": "trivy fs --scanners vuln,secret,misconfig ./",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "1.13.2",
    "next": "16.0.3",
    "next-themes": "0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sonner": "2.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@changesets/cli": "2.29.7",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@commitlint/types": "20.0.0",
    "@tailwindcss/postcss": "4.1.17",
    "@types/node": "24.10.1",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "husky": "9.1.7",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "react-hook-form": "7.66.1",
    "rimraf": "6.1.0",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3"
  }
}
